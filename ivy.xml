        <dependency org="org.apache.tomcat" name="tomcat-el-api" rev="8.0.21" conf="build->default" />
        <dependency org="org.apache.tomcat" name="tomcat-jni" rev="8.0.21" conf="build->default" />
        <dependency org="org.apache.tomcat" name="tomcat-jsp-api" rev="8.0.21" conf="build->default" />
        <dependency org="org.apache.tomcat" name="tomcat-juli" rev="8.0.21" conf="build->default" />
        <dependency org="org.apache.tomcat" name="tomcat-servlet-api" rev="8.0.21" conf="build->default" />
        <dependency org="org.apache.tomcat" name="tomcat-util" rev="8.0.21" conf="build->default" />
        <dependency org="org.apache.tomcat" name="tomcat-util-scan" rev="8.0.21" conf="build->default" />
