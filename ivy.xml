        <dependency org="org.apache.tomcat" name="tomcat-annotations-api" rev="8.0.21" conf="build->default" />
        <dependency org="org.apache.tomcat" name="tomcat-api" rev="8.0.21" conf="build->default" />
        <dependency org="org.apache.tomcat" name="tomcat-catalina" rev="8.0.21" conf="build->default" />
